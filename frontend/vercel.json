{
    "rewrites": [
      {
        "source": "/css/(.*)",
        "destination": "/docs/css/$1"
      },
      {
        "source": "/img/(.*)",
        "destination": "/docs/img/$1"
      },
      {
        "source": "/",
        "destination": [
          "/docs/index.html",
        "/docs/portfolio.html"
        ]
      }
    ]
  }
  